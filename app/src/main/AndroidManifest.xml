<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    package="com.example.zenitka.taskmanager">    <uses-permission android:name="android.permission.INTERNET" />    <application        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:roundIcon="@mipmap/ic_launcher_round"        android:supportsRtl="true"        android:theme="@style/Theme.AppCompat.Light.NoActionBar">        <activity android:name=".TeamTaskEdit"></activity>        <activity android:name=".ProjectEdit" />        <activity android:name=".ProjectList" />        <activity android:name=".TeamEdit" />        <activity android:name=".TeamList" />        <activity android:name=".RegLog.RegisterActivity" />        <activity android:name=".RegLog.LoginActivity" />        <activity            android:name=".TaskList"            android:label="@string/app_name">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity android:name=".TaskDescription" />    </application></manifest>